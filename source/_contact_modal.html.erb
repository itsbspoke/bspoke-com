<div id="contact-modal" style="display:none;">
  <h4>Contact us</h4>
  <div class="ss-form"><form action="http://spreadsheets.google.com/formResponse?formkey=dGRKLURSSUtrcUs1QnR3ME52bEhlbUE6MQ&amp;ifq" method="POST" id="ss-form">

      <table>
        <tr>
          <td>
            <div class="errorbox-good">
              <div class="ss-item ss-item-required ss-text"><div class="ss-form-entry"><label class="ss-q-title" for="entry_0">Name
                    <span class="ss-required-asterisk">*</span></label>
                  <label class="ss-q-help" for="entry_0"></label>
                  <input type="text" name="entry.0.single" value="" class="ss-q-short" id="entry_0"></div></div></div>
          </td>
          <td rowspan="2">
            <div class="errorbox-good">

              <div class="ss-item ss-item-required ss-paragraph-text"><div class="ss-form-entry"><label class="ss-q-title" for="entry_2">Note
                    <span class="ss-required-asterisk">*</span></label>
                  <label class="ss-q-help" for="entry_2"></label>
                  <textarea name="entry.2.single"  class="ss-q-long" id="entry_2"></textarea></div></div></div>
          </td>
        </tr>
        <tr>
          <td>
            <div class="errorbox-good">
              <div class="ss-item ss-item-required ss-text"><div class="ss-form-entry"><label class="ss-q-title" for="entry_1">Email
                    <span class="ss-required-asterisk">*</span></label>
                  <label class="ss-q-help" for="entry_1"></label>
                  <input type="text" name="entry.1.single" value="" class="ss-q-short" id="entry_1"></div></div></div>
          </td>
        </tr>
        <tr>
          <td colspan="2" class="submit">
            <div class="ss-item ss-navigate"><div class="ss-form-entry">
                <input type="submit" name="submit" value="Send"></div></div>
          </td>
        </tr>
      </table>

      <input type="hidden" name="pageNumber" value="0">
      <input type="hidden" name="backupCache" value="">

    </form>
    <script type="text/javascript">

(function() {
  var error = false;
  var divs = document.getElementById('ss-form').
    getElementsByTagName('div');
  for (var j in divs) {
    if (divs[j].className == 'errorbox-bad') {
      error = true;
      divs[j].lastChild.firstChild.lastChild.focus();
      break;
    }
  }
  if (!error) {
    for (var i in divs) {
      if (divs[i].className == 'ss-form-entry' && divs[i].
          firstChild.className == 'ss-q-title') {
        divs[i].lastChild.focus();
      break;
      }
    }
  }
})();
  </script></div>
</div>
